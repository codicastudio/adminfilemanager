{
    "name": "codicastudio/file-manager",
    "description": "A random Codica Studio package.",

    "homepage": "https://github.com/codicastudio/file-manager",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "mbrown@codicastudio.com",
            "homepage": "https://codicastudio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "9.0"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\Filemanager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Infinety\\Filemanager\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinety\\Filemanager\\FilemanagerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
